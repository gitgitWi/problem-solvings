{
  "compilerOptions": {
    "target": "ESNext",
    "outDir": "./dist",
    "module": "ESNext",
    "allowJs": true,
    "checkJs": true,
    "sourceMap": false,
    "importsNotUsedAsValues": "remove",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "alwaysStrict": true,
    "useUnknownInCatchVariables": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@utils/*": ["./src/utils/*"]
    }
  },
  "include": ["./solutions", "./solving", "./public", "./.private", "./src"],
}
